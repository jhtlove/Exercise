package my_sudoku;
/***
 * 数独游戏
 *关键是如何产生有解的数字矩阵？算法的可扩展性（分治递归？）
 *
 */
public class Sudoku
{
	private int size = 9;//9 * 9 的数独游戏

	private class SubDiamonds
	{
		int size;//子方块 3 * 3
		int subArr[][];
		
		SubDiamonds()
		{
			size = 3;
			subArr = new int[size][size];
		}
	}
	
	public static void create()
	{
		int subArr[][] = new int[3][3]; //一个子方块
	}
	
	public static void main(String[] args)
	{
		

	}

}
