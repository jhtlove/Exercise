package basic_level;

public class LastStringLength
{
	private String s = "hello world sunny";
	private int num = 0;
	
	public int count()
	{
		String ss[] = s.split(" ");
		String temp = ss[ss.length - 1];
		num = temp.length();
		return num;
	}
	
	public static void main(String[] args)
	{
		// TODO Auto-generated method stub

	}

}
