package basic_level;

public class StringMatch
{

	private String s1 = "abcd";
	private String s2 = "acd";
	private int num = 0;
	
	public int maxMatch()
	{
		char ss1[] = s1.toCharArray();
		char ss2[] = s2.toCharArray();
		char temp1 [];
		char temp2 [];
		int max = 0;
		if(ss1.length > ss2.length)
		{
			temp1 = ss1;
			temp2 = ss2;
		}
		else
		{
			temp1 = ss2;
			temp2 = ss1;
		}
		int i = 0,j = 0;
		for(i=0;i<temp1.length;i++)
		{
			for(j=0;j<temp2.length;j++)
			{
				
			}
		}
		
		
		return num;
	}
	
