package division_recursion;

public class BinarySearch
{

	public static int bSearch(int a[],int s,int t,int x)
	{
		int mid = (s + t) / 2;
		if(a[mid] == x) return mid;
		else if(a[mid] > x)
		{
			bSearch(a,s,mid - 1,x);
		}
		else 
			bSearch(a,mid + 1,t,x);
		return -1;//Œ¥’“µΩ
	}

	
	public static void main(String[] args)
	{
		

	}

}
