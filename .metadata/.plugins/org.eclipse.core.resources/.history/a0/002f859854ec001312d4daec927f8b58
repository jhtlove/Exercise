package testquestion;

public class AddInList
{
	private Node head = new Node();
	
	public void initia(int num)
	{
		int temp = num;
		int n = 0;
		Node index = head;
		do
		{
			n = temp % 10;
			Node n = new Node(n);
			temp = temp / 10;
		}while(temp != 0);
	}
	
	public static void main(String[] args)
	{
		
	}

}

class Node
{
	private int oneBit;
	private Node next;
	
	Node()
	{
		oneBit = 0;
		next = null;
	}
	
	Node(int oneBit)
	{
		if(oneBit >=0 && oneBit <=9)
			this.oneBit = oneBit;
		next = null;
	}
	
}
